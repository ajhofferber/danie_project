// Header cart

.header-cart-wrapper-inner {
  display: inline;
  padding-bottom: 30px;
}

.header-cart {
  position: absolute;
  top: calc(100% - 35px);
  right: 0;
  z-index: 1000;
  width: 300px;
  text-align: left;
  background-color: $colorBackground;
  line-height: 1.6;
  opacity: 0;
  visibility: hidden;
  margin-top: 10px;
  transition: all 0.2s ease;

  {% if settings.nav_type == "regular" %}
    top: calc(100% - 10px);
  {% endif %}

  &.animating {
    opacity: 0;
    visibility: visible;
  }

  &.visible {
    opacity: 1;
    visibility: visible;
    margin-top: 0;
  }

  .header-cart-wrapper-inner:hover & {
    visibility: visible;
    opacity: 1;
    margin-top: 0;
  }

  @media screen and (max-width: $break-tablet) { display: none; }
}

.header-cart-empty {
  height: 48px;
  padding-bottom: 20px;
  margin: 0;
  text-align: center;
  overflow-y: hidden;
  transition: all 0.2s ease;

  &.hidden {
    height: 0;
    padding-bottom: 0;
  }
}

.header-cart-items {
  padding: 20px 20px 0;
  border: 1px solid $borderColor;
  border-bottom: 0;

  {% if settings.use_sticky %}
    overflow-y: auto;
    max-height: 50vh;
  {% endif %}
}

.header-cart-item {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  padding-bottom: 20px;
  overflow-y: hidden;

  &.removed {
    height: 0 !important;
    padding-bottom: 0;
    opacity: 0;
    transition: all 0.2s ease-out;
  }

  img { display: block; }

  h3 {
    margin-bottom: 0;
    font-size: 1.3rem;
  }

  .money {
    color: $colorTextBody;
    font-size: 1.3rem;
  }
}

.header-cart-item-image {
  width: 80px;
}

.header-cart-item-info {
  width: calc(100% - 90px);
  margin-left: 10px;

  h3 {
    text-transform: none;
    letter-spacing: 0;
  }

  span {
    display: block;
  }
}

.header-cart-item-options {
  margin-top: 5px;

  strong {
    color: $colorTextBody;
    font-weight: 400;
    text-transform: none;
    letter-spacing: 0;
  }

  input {
    display: inline;
    width: 22px;
    border: 0;
    border-bottom: 1px solid $borderColor;

    &[type=number]::-webkit-inner-spin-button,
    &[type=number]::-webkit-outer-spin-button {
      -webkit-appearance: none;
      margin: 0;
    }
  }

  button {
    @extend a;
    padding: 0;
    margin-left: 2px;
    border: 0;
    outline: 0;
    background-color: transparent;
    text-decoration: underline;
    color: $colorPrimary;
    cursor: pointer;
  }
}

.header-cart-footer {
  display: flex;
  justify-content: space-between;
  padding: 20px;
  border: 1px solid $borderColor;
  border-bottom: 0;
}
