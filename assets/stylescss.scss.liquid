  height:100%;
  z-index:-1000;
  visibility:hidden;
  opacity:0;
  z-index:9999;
  background: rgba(0, 0, 0, 0.8);
  @include transition(all 300ms ease-out);
  &.active{
    opacity:1;
    visibility:visible;
