/*============================================================================
  #outerFrame
==============================================================================*/

{%if settings.use_page_border%}
$frameThicknes:{{settings.page_border_thickness}};

header{
   padding-top:$frameThicknes;
}

footer{
   padding-bottom:$frameThicknes;
}

#frame{
  &.popOutFix{
    opacity:0;
    visibility:hidden;
  }
  span{
    background:$colorPrimary;
    z-index:1000;
    &:nth-child(odd){
      left:0;
      width:100%;
      height:$frameThicknes;
    }
    &:nth-child(even){
      width:$frameThicknes;
      height:100%;
      top:0;
    }
    &:nth-child(1){
      top:0;
      position:fixed;
    }
    &:nth-child(2){
      left:0;
      position:fixed;
    }
    &:nth-child(3){
      bottom:0;
      position:fixed;
    }
    &:nth-child(4){
      right:0;
      position:fixed;
    }
  }
}

// frames - mobile

@media screen and (max-width: $break-grid) {

  header{
   padding-top:$frameThicknes / 2;
}

  footer{
     padding-bottom:$frameThicknes / 2;
  }

  #frame{
    span{
      &:nth-child(odd){
        height:$frameThicknes / 2;
      }
      &:nth-child(even){
        width:$frameThicknes / 2;
      }
    }
  }

}

@media screen and (max-width: $break-mobileLandscape) {

  #frame{
    display:none;
  }

  header{
   padding-top:0;
  }

  footer{
   padding-bottom:0;
  }
}
{%endif%}

