/*============================================================================
#Gen Styles
============================================================================*/

html{font-size: 62.5%;}

body{
  font-family:$mainFontStack;
  font-size:1.4rem;
  color:$colorTextBody;
  position:relative;
  background:$colorBackground;
  @include font-smoothing(antialiased);
}

main{
  opacity:0;
}

svg *{
  stroke:$colorPrimary;
}

svg path{
  fill:$colorPrimary;
}

::-moz-selection {
  background: $colorPrimary;
  color:#fff;
}

::selection {
  background: $colorPrimary;
  color:#fff;
}

p{
  line-height:2.8rem;
}

label,a,p,h1,h2,h3,h4,h5,h6,h7{
  word-break: break-word;
}

h1,h2,h3,h4,h5,h6,h7{
  color: $colorPrimary;
  margin:0 0 20px;
}

.clearTitle{
  &:after{
    content:'';
    width:30px;
    height:6px;
    background:$colorPrimary;
    margin:30px auto;
    display:block;
  }
}

hr{
  max-width:$maxWidth - 40px;
  margin:0 auto;
  border:none;
  border-bottom:1px solid $borderColor;
}

a:hover{
  color:darken($colorPrimary,10%);
}

@media screen and (max-width: $break-grid) {
  hr{
    max-width:calc(100% - 40px);
  }
}

/*============================================================================
  #RTE
==============================================================================*/
.rte {
  @include acntFontStack;
  line-height:2.4rem;
  &.leftAlign{
    text-align:left;
  }

  p,
  ul,
  ol,
  table {
    margin:0 0 20px;
    line-height: 2.4rem;
  }

  ul {
    ul {
      margin-bottom: 0;
    }
  }

  a {
    color: $colorPrimary;
    border-bottom: 1px solid $colorPrimary;
    &:hover,
    &:focus {
      border-bottom: 1px solid;
    }
  }

  h1, h4, h5, h6 {
    color: $colorPrimary;
    margin-top: 20px;
    margin-bottom: 20px;
  }

  h2, h3 {
    color: $colorPrimary;
    margin-top: 20px;
    margin-bottom: 20px;
  }

  h1, h2, h3, h4, h5, h6 {
