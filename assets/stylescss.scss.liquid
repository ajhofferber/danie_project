#Typeography
============================================================================*/

@mixin mainFontStack {
  font-family: $mainFontStack;

}

@mixin acntFontStack {
  font-family: $accentFontStack;
}

@mixin metaFontStack {
  font-family: $metaFontStack;
}

{% if settings.typekit != blank and settings.typekit-font-header != blank %}
  {% capture main_family %}{{ settings.typekit-font-header }}{% endcapture %}
  {% assign main_weight = 400 %}
{% elsif settings.type_main_family contains 'Google' %}
  {% assign type_main_parts = settings.type_main_family | split: '_' %}
  {% assign type_main_name = type_main_parts[1] %}
  {% capture main_family %}"{{ type_main_name | split: ':' | first | replace: '+', ' ' }}"{% if type_main_parts.last == 'serif' %}, serif {% else %}, "HelveticaNeue", "Helvetica Neue", sans-serif{% endif %}{% endcapture %}
  {% assign main_weight = type_main_parts[2] | split: ',' %}
{% else %}
  {% assign main_family = settings.type_main_family %}
  {% assign main_weight = 400 %}
{% endif %}

{% if settings.typekit != blank and settings.typekit-font-body != blank %}
  {% capture accent_family %}{{ settings.typekit-font-body }}{% endcapture %}
  {% assign accent_weight = 700 %}
{% elsif settings.type_accent_family contains 'Google' %}
  {% assign type_accent_parts = settings.type_accent_family | split: '_' %}
  {% assign type_accent_name = type_accent_parts[1] %}
  {% capture accent_family %}"{{ type_accent_name | split: ':' | first | replace: '+', ' ' }}"{% if type_accent_parts.last == 'serif' %}, serif {% else %}, "HelveticaNeue", "Helvetica Neue", sans-serif{% endif %}{% endcapture %}
  {% assign accent_weight = type_accent_parts[2] | split: ',' %}
{% else %}
  {% assign accent_family = settings.type_accent_family %}
  {% assign accent_weight = 700 %}
{% endif %}

{% if settings.typekit != blank and settings.typekit-font-logo != blank %}
  {% capture logo_family %}{{ settings.typekit-font-logo }}{% endcapture %}
  {% assign accent_weight = 700 %}
{% elsif settings.type_logo_family contains 'Google' %}
  {% assign type_logo_parts = settings.type_logo_family | split: '_' %}
  {% assign type_logo_name = type_logo_parts[1] %}
  {% capture logo_family %}"{{ type_logo_name | split: ':' | first | replace: '+', ' ' }}"{% if type_logo_parts.last == 'serif' %}, serif {% else %}, "HelveticaNeue", "Helvetica Neue", sans-serif{% endif %}{% endcapture %}
  {% assign logo_weight = type_logo_parts[2] | split: ',' %}
{% else %}
  {% assign logo_family = settings.type_logo_family %}
  {% assign logo_weight = 700 %}
{% endif %}

{% if settings.typekit != blank and settings.typekit-font-meta != blank %}
  {% capture meta_family %}{{ settings.typekit-font-meta }}{% endcapture %}
  {% assign accent_weight = 700 %}
{% elsif settings.type_meta_family contains 'Google' %}
  {% assign type_meta_parts = settings.type_meta_family | split: '_' %}
  {% assign type_meta_name = type_meta_parts[1] %}
  {% capture meta_family %}"{{ type_meta_name | split: ':' | first | replace: '+', ' ' }}"{% if type_meta_parts.last == 'serif' %}, serif {% else %}, "HelveticaNeue", "Helvetica Neue", sans-serif{% endif %}{% endcapture %}
  {% assign meta_weight = type_meta_parts[2] | split: ',' %}
{% else %}
  {% assign meta_family = settings.type_meta_family %}
  {% assign meta_weight = 700 %}
{% endif %}

