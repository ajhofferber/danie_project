    width:100%;
    height:100vh;
    position:absolute;
    top:0;
    left:0;
    z-index:0;
    &:not(.fixedPosition){
      background-attachment:fixed;
    }
    &.fixedPosition{
      width:50%;
    }
    &.screenLock{
      position:fixed;
      left:0;
      z-index:-1;
    }
  }

  .content{
    width:50%;
    height:100%;
    top:0;
    right:0;
    padding:80px;
