///////////////
//REGULAR NAV//
///////////////
#regularMenu{
 {%if settings.nav_type == 'burger'%}
  display:none;
  {%endif%}
  @include a_easeOne;
  text-align:center;
  width:100%;
  color:$colorPrimary;
  position:relative;

  .grid{
    height:auto;
    line-height:1;
    position:static;
    margin-top:0;
  }
