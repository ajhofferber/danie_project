.parallaxListing{
  position:relative;
  padding:0 0 80px;

  li{
    background:transparent;
    @include prefixer(backface-visibility, none, webkit spec);
    width:100%;
    padding:80px 0;
    text-align:center;

    a{
      display:block;
      width:100%;
      position:relative;

    }

    .imgWrap{
      padding:0;
      position:relative;
      margin:0 auto;
      z-index:1;
      width:50%;
    }
    p.genHeadingXL{
       margn:0;
      line-height:5.4rem;
      position:absolute;
      top:50%;
      left:0;
      width:100%;
      z-index:99;

    }

    img{
      max-width:100%;
      &:not(.abImage){
        position:relative;
        z-index:1;
        @include prefixer(box-shadow,  0px 6px 25px -10px rgba(0,0,0,0.75), webkit spec);
      }
    }

    .abImage{
      position:absolute;
      z-index:0;
      &.left{
        left:calc(0% - (50% - 20px));
      }
      &.right{
        right:calc(0% - (50% - 20px));
      }
    }

    &:nth-child(odd){
      .abImage.left{
        bottom:20%;
       }
      .abImage.right{
        top:20%;
       }
    }

    &:nth-child(even){
      .abImage.left{
        top:20%;
       }

      .abImage.right{
        bottom:20%;
      }
    }


    &:nth-child(4n-7){
      .abImage.left{
        margin:-70px -85px 0 0;
      }

      .abImage.right{
        margin:0 0 -80px -62px;
      }


    }

    &:nth-child(4n-6){
      .abImage.left{
        margin:-100px -85px 0 0;
      }

      .abImage.right{
        margin:0 0 -70px -12px;
      }


    }


     &:nth-child(4n-3){
      .abImage.left{
        margin:-10px -45px 0 0;
      }

      .abImage.right{
        margin:0 0 -20px -92px;
      }


    }
  }
}

