/*============================================================================
  #header
==============================================================================*/

{%if settings.nav_type == 'burger'%}
$clearValue:121px;
$headerMarginTop:0px;
$headerHeight:120px;
$headerLogoHeight:90px;
{%else%}
$clearValue:121px;
$headerHeight:70px;
$headerLogoHeight:70px;
$headerMarginTop:0px;
{%endif%}

.ctop{
  margin-top:$clearValue;
}

.rtop{
  margin-top:-$clearValue;
}

header{
  @include a_easeOne;
  width:100%;
  border-bottom:1px solid transparent;
  position:relative;
  z-index:999;

  .grid{
    height:$headerHeight;
    line-height:$headerHeight;
    margin-top:$headerMarginTop;
    position:relative;
    display:inline-block;
  }
  &.locked{
    position:fixed;
    top:0;
    left:0;
  }

  svg{
    vertical-align:middle;
    height:20px;
  }

  svg *{
    @include a_easeOne;
    fill:$colorPrimary;
    stroke:none;
  }

  svg path{
    @include a_easeOne;
    fill:$colorPrimary;
    stroke:none;
  }

  .leftMenu{
    padding-right:0;
    a{
      display:inline-block;
      vertical-align:middle;
      margin-right:40px;
      width:20px;
      {%if settings.nav_type != 'burger'%}
      &:nth-child(3){
        margin-right:40px;
      }
      {%else%}
      &:nth-child(2){
        margin-right:40px;
      }
      {%endif%}
    }
  }

  .rightMenu{
    color:$colorPrimary;
    font-family:$metaFontStack;
    padding-left:0;
    text-align:right;
    font-size:1.2rem;
    text-transform:uppercase;
    letter-spacing:1px;
    a{
      @include a_easeOne;
    }

    > div {
      position: relative;
      display: inline-block;
      margin-left:40px;
    }
  }

  .logo{
    color:$colorPrimary;
    font-family:$logoFontStack;
    text-align:center;
    font-weight:$logoBoldOption;
    font-size:3.1rem;
    margin: auto;
    max-width: 100%;
    max-height:100%;
    overflow:hidden;
    padding:0;

    img{
      max-width:$logoMaxWidth;;
      max-height:$headerLogoHeight;
      display:block;
      margin:0 auto;
    }
  }

  &:not(.noHover):hover,&.active,&.noTran,&.scrolled{
    background:$colorBackground;
    border-bottom: 1px solid $colorBackground;
    box-shadow: 0 1px 01px -1px rgba(0,0,0,0.3);

    svg *{
      fill:$colorTextMetaLighten;
    }

    svg path{
    fill:$colorTextMetaLighten;
    }

    .rightMenu{
      color:$colorTextMeta;
    }

    .burgerNav:not(.active) span{
      background:$colorTextMetaLighten;
    }

    #regularMenu{
      border-color:$borderColor;
    }
  }
}

.burgerNav{
  {%if settings.nav_type != 'burger'%}
  display:none!important;
  {%else%}
   display:inline-block;
  {%endif%}
  height:20px;
  width:20px;
  position:relative;
  cursor:pointer;
  z-index:999;
  vertical-align:middle;
  @include transition(transform 300ms ease);
  @include prefixer(transition-delay,300ms, ms webkit spec);
  span{
    @include transition(all 300ms ease);
    @include prefixer(transition-delay,0ms, ms webkit spec);

    background:$colorPrimary;
    position: absolute;
    left:0;
    width: 20px;
    height: 1px;
    &:nth-child(1){
      top: 25%;

    }
    &:nth-child(2){
      top: 50%;
    }
    &:nth-child(3){
      top: 75%;

    }
  }

  &.active{
    @include prefixer(transform, rotate(90deg), ms webkit spec);
    @include prefixer(transition-delay,0ms, ms webkit spec);
    span{
      @include prefixer(transition-delay,300ms, ms webkit spec);
      background:#fff;
      &:nth-child(1){
        top:50%;
        @include prefixer(transform, matrix(0.7071, -0.7071, 0.7071, 0.7071, 0, 0), ms webkit spec);
      }
      &:nth-child(2){
        opacity:0;
      }
      &:nth-child(3){
        top:50%;
        @include prefixer(transform, matrix(0.7071, 0.7071, -0.7071, 0.7071, 0, 0), ms webkit spec);
      }
    }
  }
}


//////////////
//BURGER NAV//
//////////////
#burgerMenu {
  {%if settings.nav_type != 'burger'%}
  display:none;
  {%endif%}
  font-size:1.6rem;
  font-family: $metaFontStack;
  background: $colorPrimary;
  color: #fff;
  position: absolute;
  top: 50%;
  left: 0;
  margin: -21px 0 0 6px;
  width: 20%;
  padding: 50px 15px 5px;
  z-index: 998;
  visibility: hidden;
  opacity: 0;
  border-radius:2px;
  @include transition(all 300ms ease-out);
  @include prefixer(transform,translateY(-5px), ms webkit spec);
  @include prefixer(transition-delay,300ms, ms webkit spec);
  &.active {
    @include prefixer(transform,translateY(0), ms webkit spec);
    @include prefixer(transition-delay,0ms, ms webkit spec);
    visibility: visible;
    opacity: 1;
  }

  a {
    margin:0;
    display: block;
	font-weight: bold;
    padding: 0 0 10px;
    line-height:1;
    width:100%;
    font-size:1.4rem;
    &:hover{
      color:#fff;
    }
  }

  .hasDrop {
    a.parentLink{
      &:after {
        font: normal normal normal 12px/1 FontAwesome;
        content: "\f078 ";
        display:inline-block;
        vertical-align:top;
        line-height:1.2rem;
        float:right;
        font-size:1rem;
      }
    }
    ul{
      display:none;
      li{
        padding:0 0 0 10px;
      }
    }
    &.active{
      a.parentLink:after {
        content: "\f077";
      }
      ul{
        display:block;
      }
    }
  }
}



